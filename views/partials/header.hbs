<!-- Button Login modal -->
<button type="button" id="modalLogin" class="btn btn-primary btn-lg d-none" data-toggle="modal"
    data-target="#modelIdLogin">
    Launch
</button>

<!-- Modal Login -->
<div class="modal fade" id="modelIdLogin" tabindex="-1" role="dialog" aria-labelledby="modelTitleId" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 id="modalTitle" class="modal-title">Đăng nhập</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div id="TH_errorMessage"></div>
            <form action="">
                <div class="form-group">
                    <label for="phoneNumber"></label>
                    <input type="text" class="form-control" autocomplete="username" maxlength="11" required="required" 
                    id="TH_phoneNumber" aria-describedby="helpId"
                        placeholder="Nhập số điện thoại">
                </div>

                <div class="form-group">
                    <label for="password"></label>
                    <input type="password" class="form-control" autocomplete="current-password" id="TH_password" aria-describedby="helpId"
                        placeholder="Nhập mật khẩu">
                </div>
            </form>

                <button class="btn btn-success" id="showModalRegister" style="cursor:pointer"></button>

            </div>
            <div class="modal-footer">
                <button type="button" id="TH_close_modal_login" class="btn btn-outline-danger"
                    data-dismiss="modal">Đóng</button>
                <button type="button" id="submitLogin" class="btn btn-outline-primary">Đăng nhập</button>
            </div>
        </div>
    </div>
</div>

<!-- Modal Thông báo -->
    <button type="button" id="modal_ThongBao" class="btn btn-primary btn-lg d-none" data-toggle="modal" data-target="#modelThongbao">
